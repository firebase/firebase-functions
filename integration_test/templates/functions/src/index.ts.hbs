import * as admin from "firebase-admin";

// Initialize admin SDK
const projectId = process.env.PROJECT_ID || process.env.GCLOUD_PROJECT || "{{projectId}}";

if (!admin.apps.length) {
  try {
    admin.initializeApp({
      projectId: projectId
    });
  } catch (error) {
    console.log("Admin SDK initialization skipped:", error.message);
  }
}

// Export all generated test suites
{{#each suites}}
export * from "./{{this.version}}/{{this.service}}-tests"; // {{this.name}}
{{/each}}